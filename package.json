{
  "name": "vue3-vite-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently -s -n dev: 'npm:dev:*'",
    "dev:vite": "vite",
    "dev:db": "json-server --watch server/db.json --routes server/routes.json --port 3001",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "test": "concurrently -s -n test: 'npm:test:*'",
    "test:unit": "vitest --environment jsdom --run",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0-alpha.42",
    "@vuelidate/validators": "^2.0.0-alpha.30",
    "@vueuse/core": "^8.7.5",
    "axios": "^0.27.2",
    "concurrently": "^7.1.0",
    "primeicons": "^5.0.0",
    "primevue": "^3.12.6",
    "rxjs": "^7.8.1",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "vue3-mq": "^3.1.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.20",
    "autoprefixer": "^10.4.5",
    "jsdom": "^19.0.0",
    "json-server": "^0.17.0",
    "lint-staged": "^13.0.2",
    "markdownlint-cli2": "^0.4.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.5",
    "vitest": "^0.9.3"
  }

}
